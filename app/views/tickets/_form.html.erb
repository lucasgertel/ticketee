<%= form_for [@project, @ticket] do |f| %>
	<ul>
	<% @ticket.errors.full_messages.each do |msg| %>
  	<li><%= msg %></li>
  <% end %>
	</ul>
	<p>
		<%= f.label :title %>
		<%= f.text_field :title %>
	</p>
	<p>
		<%= f.label :description %>
		<%= f.text_field :description %>
	</p>
	<%= f.submit %>
<% end %>